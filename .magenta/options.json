{
  "container": {
    "devcontainer": "docker/Dockerfile",
    "workspacePath": "/workspace",
    "installCommand": "npm ci",
    "volumeOverlays": ["node_modules"]
  },
  "commandConfig": {
    "commands": [
      ["npx", "tsc", "--noEmit"],
      ["npx", "tsgo", { "type": "restAny" }],
      ["npx", "vitest", "run", { "type": "restAny" }]
    ]
  }
}
